    "marketprice": {
        "name": "marketprice",
        "addr": [
            "tcp@192.168.18.43:7416"
        ],
        "max_pkg_size": 2000000
    },
    "readhistory": {
        "name": "readhistory",
        "addr": [
            "tcp@192.168.18.44:7424"
        ],
        "max_pkg_size": 2000000
    },
    "deals": {
